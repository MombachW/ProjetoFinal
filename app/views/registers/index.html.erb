<p id="notice"><%= notice %></p>

<h1>Registros</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Numero</th>
      <th>Cidade</th>
      <th>Cpf</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registers.each do |register| %>
      <tr>
        <td><%= register.name %></td>
        <td><%= register.number %></td>
        <td><%= register.city %></td>
        <td><%= register.cpf %></td>
        <td><%= link_to 'Mostrar', register %></td>
        <td><%= link_to 'Editar', edit_register_path(register) %></td>
        <td><%= link_to 'Apagar', register, method: :delete, data: { confirm: 'VocÃª tem certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo registro', new_register_path %>
